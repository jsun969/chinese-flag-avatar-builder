{
  "expo": {
    "name": "庆国庆头像生成器",
    "slug": "chinese-flag-avatar-builder",
    "version": "0.1.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "githubUrl": "https://github.com/jsun969/chinese-flag-avatar-builder",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.jsun969.chineseflagavatarbuilder",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#c40000"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}